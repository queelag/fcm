{
  "author": "Dario Sechi",
  "dependencies": {
    "google-auth-library": "^9.14.2",
    "http_ece": "^1.2.1",
    "long": "^5.2.3",
    "protobufjs": "^7.4.0"
  },
  "devDependencies": {
    "@aracna/core": "^1.1.92",
    "@microsoft/api-extractor": "^7.47.9",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "glob": "^11.0.0",
    "prettier": "^3.3.3",
    "protobufjs-cli": "^1.1.3",
    "ts-proto": "^2.2.3",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.9",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "exports": {
    "types": "./index.d.ts",
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.js",
  "name": "@aracna/fcm",
  "packageManager": "pnpm@9.1.0",
  "peerDependencies": {
    "@aracna/core": ">= 1.1.0"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 160,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "repository": "https://github.com/aracna/fcm",
  "scripts": {
    "build": "rm -rf dist && node esbuild.mjs && cp -rf src/assets dist/assets && pnpm tsc && pnpm api-extractor run --local",
