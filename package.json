    "generate:proto-ts": "node scripts/generate-proto-ts.mjs && pnpm prettier --write src/assets",
    "prepublish": "pnpm test && pnpm build && cp LICENSE package.json README.md dist",
    "test": "vitest run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "index.d.ts",
  "version": "1.0.28"
}
